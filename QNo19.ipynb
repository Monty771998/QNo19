   "source": [
    "l=[11,48,68,48,42,95,74,23,10,54,15]\n",
    "def calculate_mean(l):\n",
    "    total=0\n",
    "    for i in l:\n",
    "        total+=i\n",
    "        count = len(l)\n",
    "        mean = total / count\n",
    "    return mean"
   ]
  },
